# OpenRouter credentials for Coordinator/Fixer LLM calls.
OPENROUTER_API_KEY=sk-or-v1-your-key
# Model names passed to OpenHands (OpenRouter routing handles the provider).
COORDINATOR_OPENROUTER_MODEL=minimax/minimax-m2.1
FIXER_OPENROUTER_MODEL=minimax/minimax-m2.1
# Toggle Docker workspace usage; set false to run locally.
COORDINATOR_USE_DOCKER=true
# Base image for DockerWorkspace when no server image is set.
DOCKER_BASE_IMAGE=python:3.11-slim
# Custom agent-server image with DelegateTool preloaded.
DOCKER_SERVER_IMAGE=pycve-agent-server:delegate
# Include DelegateTool in the Coordinator's tool list for Docker runs.
DOCKER_ENABLE_DELEGATE=true
